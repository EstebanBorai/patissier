{
  "name": "patissier",
  "version": "0.2.0",
  "description": "HTTP Cookie builder library following the RFC6265",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EstebanBorai/patissier.git"
  },
  "keywords": ["http", "cookie", "builder"],
  "author": "Esteban Borai <estebanborai@gmail.com> (https://estebanborai.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EstebanBorai/patissier/issues"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "check": "tsc --noEmit && rome check ./src",
    "coverage": "vitest --config ./vitest.config.ts run --coverage",
    "format": "rome format ./src",
    "test": "vitest --config ./vitest.config.ts"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@vitest/coverage-c8": "^0.31.1",
    "c8": "^7.11.3",
    "rollup": "^3.23.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "rome": "^12.1.2",
    "tslib": "^2.4.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  }
}
