{
  "name": "patissier",
  "version": "0.1.0",
  "description": "HTTP Cookie builder library following the RFC6265",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EstebanBorai/patissier.git"
  },
  "keywords": [
    "http",
    "cookie",
    "builder"
  ],
  "author": "Esteban Borai <estebanborai@gmail.com> (https://estebanborai.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EstebanBorai/patissier/issues"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "check": "tsc --noEmit",
    "coverage": "vitest --config ./vitest.config.ts run --coverage",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. . && eslint --fix .",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "test": "vitest --config ./vitest.config.ts"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "c8": "^7.11.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "rollup": "^2.75.5",
    "rollup-plugin-typescript2": "^0.32.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "vitest": "^0.12.9"
  }
}
